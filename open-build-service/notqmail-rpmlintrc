addFilter(".* non-standard-gid")
addFilter(".* is not allowed anymore in FHS .*")
addFilter(".* invalid-license .*")
addFilter(".* permissions-world-writable .* /var/qmail/queue/lock/trigger is packaged with world writable permissions")
addFilter(".* suse-filelist-forbidden-fhs23 /var/qmail is not allowed in FHS .*")
addFilter(".* permissions-file-setuid-bit")
addFilter(".* permissions-directory-setuid-bit .* is packaged with setuid/setgid bits .*")
addFilter(".* non-standard-dir-in-var .*")
addFilter(".* non-standard-uid")
addFilter(".* zero-length /var/qmail/queue/lock/sendmutex")
addFilter(".* file-parent-ownership-mismatch /var/qmail/queue/.* owned by .* is stored in directory owned by different user .*")
setBadness('permissions-unauthorized-file', 0)
setBadness('permissions-file-setuid-bit', 0)
