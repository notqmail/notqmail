addFilter(".* unversioned-explicit-self-provides")
addFilter(".* non-standard-gid")
addFilter(".* is not allowed anymore in FHS .*")
addFilter(".* invalid-license .*")
addFilter(".* permissions-world-writable .* /var/qmail/queue/lock/trigger is packaged with world writable permissions")
addFilter("W: suse-filelist-forbidden-fhs23 /var/qmail is not allowed in FHS .*")
addFilter("W: permissions-file-setuid-bit")
addFilter("W: permissions-directory-setuid-bit .* is packaged with setuid/setgid bits .*")
addFilter("W: non-standard-dir-in-var .*")
addFilter("W: non-standard-uid")
addFilter("W: zero-length /var/qmail/queue/lock/sendmutex")
addFilter("W: file-parent-ownership-mismatch /var/qmail/queue/.* owned by .* is stored in directory owned by different user .*")
setBadness('permissions-unauthorized-file', 0)
setBadness('permissions-file-setuid-bit', 0)
